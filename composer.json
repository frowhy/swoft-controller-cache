{
  "name": "frowhy/swoft-controller-cache",
  "type": "library",
  "keywords": [],
  "description": "",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1",
    "ext-igbinary": "*",
    "ext-swoole": ">=4.3",
    "swoft/component": "~2.0.0"
  },
  "require-dev": {
    "swoft/swoole-ide-helper": "dev-master",
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Frowhy\\SwoftControllerCache\\": "src/"
